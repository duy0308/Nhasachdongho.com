    <div class="main-wrapper ">

        <header>

            <div class="header-container first">
                @Html.Partial("_topbar")

                <div class="header">
                    <div class="container">
                        <div class="row hidden-xs">
                            @{Html.RenderAction("Logo", "Home");}
                            <!--KHUNG TÌM KIẾM-->
                            @{Html.RenderAction("MathangLoaiTK", "Home");}
                            <!-- CART INFO -->
                            @Html.Partial("_CartInfo")

                            <!-- END CART INFO -->
                        </div>
                        <div class="mobile-top-cart row hidden-lg hidden-md hidden-sm">
                            @{Html.RenderAction("LogoMobi", "Home");}
                            <div class="cart-container col-xs-8 mg-20-0">
                                <div class="shoping_cart pull-right">
                                    <div class="widget woocommerce widget_shopping_cart"><div class="widget_shopping_cart_content"></div></div>
                                </div>
                            </div>
                            <!--- SEARCH NGANG CHO MOBILE -->
                            @{Html.RenderAction("MathangLoaiTKMobi", "Home");}
                            <!---  END SEARCH NGANG CHO MOBILE -->
                        </div>
                    </div>
                </div>
                <!--MENU NGANG-->
                @{Html.RenderAction("MathangLoai", "Home");}
                <!-- END MENU NGANG-->
                <div class="clearfix"></div>
            </div>
        </header>
        <div id="content" class="site-content">
            <div id="main-content" class="bigshop_home_template">
                <!-- SLIDER -->
                @{Html.RenderAction("Slider", "Home");}
                <!-- / END SLIDER -->
                <!-- GHI CHU DICH VỤ  -->
                @Html.Partial("_Service")
                <!-- END GHI CHU DICH VỤ  -->
                <!-- ADS VITRI 1 -->
                <div class="clearfix"></div>
                @Html.Partial("_AdsVitri1")
                <!-- / END ADS VITRI 1 -->
                @RenderBody()

                <!-- SHOW PRODUCT IN HOME -->
                @*@Html.Partial("_SanphamHome")*@
                <!-- /END SHOW PRODUCT IN HOME -->

            </div>

        </div>
        <!--.site-content-->
        @{Html.RenderAction("Footer", "Home");}
        <div id="back-top" class="hidden-xs"><i class="fa fa-angle-up"></i></div>
        @Html.Partial("_Socical")
    </div><!--.main wrapper-->

    <div class="popup-content" id="popup_onload">
        <div class="overlay-bg-popup"></div>
        <div class="popup-content-wrapper">
            <a class="close-popup" href="javascript:void(0)"><i class="fa fa-times"></i></a>
            <div class="popup-content-text">
                <img class="aligncenter size-full wp-image-565" src="/wp-content/uploads/2016/04/popup_img-1.jpg" alt="popup_img" />
            </div>
            <div class="newletter-form">
                <div class="widget_wysija_cont shortcode_wysija">
                    <div id="msg-form-wysija-shortcode5789c05f4bfe0-1" class="wysija-msg ajax"></div>
                    <form id="form-wysija-shortcode5789c05f4bfe0-1" method="post" action="#wysija" class="widget_wysija shortcode_wysija">
                        <p>Class aptent taciti sociosqu ad litora torquent per conubia nostra, per inceptos himenaeos.</p>
                        <p class="wysija-paragraph">


                            <input type="text" name="wysija[user][email]" class="wysija-input validate[required,custom[email]]" title="Email" placeholder="Email" value="" />



                            <span class="abs-req">
                                <input type="text" name="wysija[user][abs][email]" class="wysija-input validated[abs][email]" value="" />
                            </span>

                        </p>
                        <input class="wysija-submit wysija-submit-field" type="submit" value="Subscribe!" />
                        <input type="hidden" name="form_id" value="1" />
                        <input type="hidden" name="action" value="save" />
                        <input type="hidden" name="controller" value="subscribers" />
                        <input type="hidden" value="1" name="wysija-page" />

                        <input type="hidden" name="wysija[user_list][list_ids]" value="1" />

                    </form>
                </div>
            </div>
            <label class="not-again"><input type="checkbox" value="1" name="not-again" /><span>Do not show this popup again</span></label>
        </div>
    </div>

